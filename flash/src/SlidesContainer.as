package  {		import flash.display.MovieClip;	import flash.display.FrameLabel;	import com.epologee.development.logging.TraceLogger;	import com.epologee.development.logging.logger;	import com.epologee.navigator.Navigator;	import com.epologee.navigator.features.display.DebugConsole;		public class SlidesContainer extends MovieClip{		private var _logger:TraceLogger;		private var _navigator:Navigator;		private var _display:DebugConsole;		private var _currentLabelName:String;				public function SlidesContainer() {			stop();			_init();		}		protected function _init():void{			_logger = new TraceLogger();			_navigator = new Navigator();			_display = new DebugConsole(_navigator);			stage.addChild(_display);			_navigator.start();		}		public function goto(theLabel):void{			_currentLabelName = theLabel;			if(!this.currentLabels.some(isCurrentLabel)){				trace("no label named",theLabel);				return;			}			var slide : ASlide = getChildAt(0) as ASlide;			_navigator.remove(slide,currentLabel);			gotoAndStop(theLabel);			_navigator.add(getChildAt(0) as ASlide,theLabel);			_navigator.request(theLabel);		}		private function isCurrentLabel(element:FrameLabel, index:int, arr:Array):Boolean {            return (element.name == _currentLabelName);        }	}	}